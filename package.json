    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.8.1",
    "tsm": "^2.3.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@tinyhttp/app": "^2.5.2",
    "@tinyhttp/cors": "^2.0.1",
    "milliparsec": "^5.0.2",
    "router": "link:@types/@tinyhttp/router"
  }
